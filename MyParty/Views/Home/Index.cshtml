@model List<MyParty.ViewModels.PartyViewModel>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div>
        <p>Всем привет. Приходите на наши вечеринки</p>
        @foreach (var item in Model)
        {
            <p>
                @Html.ActionLink(item.Title, "index", "Party", new { id = item.Id}, null)
            </p>
        }

    </div>
</body>
</html>
